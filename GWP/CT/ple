plot 'energy.dat' u 1:2 w l, 'energy.dat' u 1:3 w l, 'energy.dat' u 1:4 w l, \
	'energy.dat' u 1:5 w l

set term postscript
set output 'ener.ps' 
plot 'energy.dat' u 1:2 w l  lw 2 ti 'K',  \
	'energy.dat' u 1:3 w l lw 2 ti 'V','energy.dat' u 1:4 w l lw 2 ti 'U',  \
	'energy.dat' u 1:5 w l lw 2 ti 'E'

